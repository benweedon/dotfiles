{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "workspace": {
        "checkThirdParty": false
    },
    "diagnostics": {
        "globals": [
            "_G",
            "debug",
            "error",
            "ipairs",
            "package",
            "pairs",
            "pcall",
            "string",
            "table",
            "tonumber",
            "type",
            "vim"
        ],
        "disable": [
            "redefined-local"
        ]
    },
    "runtime": {
        "version": "LuaJIT"
    }
}
